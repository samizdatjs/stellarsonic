{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "types": [
      "node"
    ],
    "target": "es5",
    "module": "commonjs",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "suppressImplicitAnyIndexErrors": false,
    "noResolve": false,
    "esModuleInterop": false
  }
}
