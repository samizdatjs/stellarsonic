{
  "name": "@samizdat/stellarsonic",
  "version": "0.7.1",
  "private": false,
  "scripts": {
    "clean": "rimraf dist",
    "commit": "git-cz",
    "start": "node ./dist/server/main.js",
    "build": "npm run build-server && npm run build-client",
    "build-server": "tsc -p ./src/server --outDir ./dist/server",
    "build-client": "webpack --env prod",
    "dev": "npm run build-server && node ./dist/server/main.js --dev=true",
    "create-post": "node ./dist/server/cli create-post",
    "create-author": "node ./dist/server/cli create-author"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/samizdatjs/stellarsonic"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
