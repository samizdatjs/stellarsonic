    "@types/webpack": "4.4.0",
    "@types/webpack-env": "1.13.0",
    "@types/yargs": "^11.0.0",
    "aurelia-webpack-plugin": "3.0.0",
    "awesome-typescript-loader": "5.2.0",
    "commitizen": "^4.0.3",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "2.1.0",
