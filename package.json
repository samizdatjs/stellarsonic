    "@ziqquratu/markdown": "^1.0.0",
    "@ziqquratu/nabu": "^1.2.0",
    "@ziqquratu/schema": "^1.3.0",
    "@ziqquratu/tashmetu": "^1.1.2",
    "@ziqquratu/terminal": "^1.0.1",
    "@ziqquratu/view": "^1.2.2",
    "@ziqquratu/ziqquratu": "^1.3.0",
    "aurelia-animator-css": "^1.0.4",
    "aurelia-bootstrapper": "2.1.1",
    "aurelia-event-aggregator": "1.0.1",
    "aurelia-framework": "1.1.5",
    "aurelia-history-browser": "1.4.0",
    "aurelia-logging-console": "1.0.0",
    "aurelia-router": "^1.4.0",
    "aurelia-templating-binding": "1.3.0",
    "aurelia-templating-resources": "1.4.0",
    "aurelia-templating-router": "1.2.0",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^6.1.0",
    "express": "^4.17.1",
    "figlet": "^1.5.0",
    "inquirer": "^7.3.3",
    "iso8601-duration": "^1.2.0",
    "showdown": "^1.8.6",
